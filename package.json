{
  "name": "webpack2-phantomjs-poc",
  "version": "1.0.0",
  "scripts": {
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail && copy package.json dist",
    "prebuild": "npm run lint && npm run test-coverage",
    "lint": "tslint ./{src,test}/**/*.ts -s node_modules/tslint-stylish -t stylish",
    "pretest": "rimraf coverage",
    "snapshot": "node config/snapshot.js",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start --browsers=Chrome",
    "test-coverage": "karma start --no-auto-watch --single-run",
    "test-ie": "karma start --browsers=IE",
    "typings": "typings"
  },
  "license": "ISC",
